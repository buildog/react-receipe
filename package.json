    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "css-loader": "^0.23.1",
    "postcss-loader": "^0.8.2",
    "precss": "^1.4.0",
    "autoprefixer": "^6.3.6",
    "del": "^2.2.0",
    "eslint": "^2.5.3",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs": "0.0.2",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1",
    "react-motion": "latest",
    "react-headroom": "latest",
    "react-youtube": "latest",
    "siriwavejs": "^2.0.1",
    "socket.io-client": "^1.1.0",
    "feathers": "2.0.0-pre.4",
    "feathers-authentication": "latest",
    "feathers-hooks": "^1.0.0-pre.4",
    "feathers-localstorage": "^0.2.0",
    "feathers-memory": "^0.6.3",
    "feathers-socketio": "^1.3.2",
    "fetch-plus": "3.6.1",
    "fetch-plus-bearerauth": "3.5.0",
    "fetch-plus-json": "3.6.0",
    "EventEmitter": "1.0.0"
  },
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "feathers": "*",
    "feathers-authentication": "*",
    "feathers-hooks": "*",
    "feathers-socketio": "*",
    "githubhook": "^1.7.1",
    "localstorage-memory": "*",
    "medium-draft": "^0.4.0",
    "react": "^15.0.0-rc",
    "react-dom": "^15.0.0-rc",
    "react-router": "^2.0.0",
