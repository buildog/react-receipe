{
  "name": "blockyc",
  "version": "1.0.0",
  "description": "blockyc aim to change how we prove the compliance process",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run prod:deploy",
    "clean": "node ./bin/clean.js",
    "server:clientside": "node ./bin/server/clientSideRendering.js",
    "server:serverside": "node ./bin/server/serverSideRendering.js",
    "dev": "npm run dev:watch",
    "dev:watch:webpack": "webpack -d --watch --colors --config ./config/webpack.clientside.dev.js",
    "dev:build": "webpack -d --colors --config ./config/webpack.clientside.dev.js",
    "dev:watch": "node ./bin/server/hotReload.js",
    "prod:build:clientside": "webpack -p --progress --colors --config ./config/webpack.clientside.prod.js",
    "prod:build:serverside": "webpack -p --progress --colors --config ./config/webpack.serverside.js",
    "prod:deploy:clientside": "npm run prod:build:clientside && npm run server:clientside",
    "prod:deploy:serverside": "npm run prod:build:serverside && npm run server:serverside",
    "prod:deploy": "npm run prod:deploy:serverside"
  },
  "author": "Adrien REMACLE <adrien@naptiv.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
